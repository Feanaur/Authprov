<h1>Greetings!</h1>

  <% form_for @picture, :html => { :multipart => true } do |f| %>
  <p>
    <b> Picture </b><br/>
    <%= f.file_field :pic %>
  </p>
<% if user_signed_in? %>
    <%current_user.pictures.each do |image| %>
      <tr>
        <td><%= image_tag image.pic.url %></td>
      </tr>
    <%end%>
  
<% end %>